{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "preinstall": "npm i nodemon -g",
    "start:watch": "nodemon -L -e ts --exec \"remix build && remix dev\"",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "start": "remix dev",
    "typecheck": "tsc",
    "db:load": "npx prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "prisma": "^4.12.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
